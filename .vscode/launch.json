    // protractor path for Mac -> /usr/local/lib/node_modules/protractor/bin/protractor
    // protractor path for Windows -> <user-home>/AppData/Roaming/npm/node_modules/protractor/bin/protractor
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Protractor Debugger",
            "program": "/usr/local/lib/node_modules/protractor/bin/protractor",
            "protocol": "inspector",
            "args": [
                "${workspaceFolder}/conf-protractor.js"
            ]
        }
    ]
}